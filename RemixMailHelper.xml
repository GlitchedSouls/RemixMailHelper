<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ../FrameXML/UI.xsd">
	<Frame name="DebugFrame" parent="UIParent" hidden="true" inherits="BasicFrameTemplateWithInset">
		<Size>
			<AbsDimension x="400" y="300"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Frames>
			<ScrollFrame name="$parentScrollFrame" inherits="UIPanelScrollFrameTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="10" y="-30"/>
					<Anchor point="BOTTOMRIGHT" x="-30" y="10"/>
				</Anchors>
				<Frames>
					<EditBox name="$parentEditBox" inherits="ChatFontNormal">
						<MultiLine>true</MultiLine>
						<Anchors>
							<Anchor point="TOPLEFT"/>
							<Anchor point="BOTTOMRIGHT"/>
						</Anchors>
					</EditBox>
				</Frames>
			</ScrollFrame>
		</Frames>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="$parentTitle" inherits="GameFontHighlight">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parentTitleBg" x="5" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>
